        <form id="lab1" method="POST" action="/cal_lab1">
            <div class="form-group row">
                <label for="r1" class="col-sm-2 col-form-label">Enter Resistance [R1]</label>
                <div class="col-sm-4">
                    <input type="number" class="form-control" id="r1" name="r1" placeholder="">
                </div>
                <label for="r1" class=" col-form-label">&#8486;</label>
            </div>

            <div class="form-group row">
                <label for="r2" class="col-sm-2 col-form-label">Enter Resistance [R2]</label>
                <div class="col-sm-4">
                    <input type="number" class="form-control" id="r2" name="r2" placeholder="">
                </div>
                <label for="r2" class=" col-form-label">&#8486;</label>
            </div>

            <div class="form-group row">
                <label for="r3" class="col-sm-2 col-form-label">Enter Resistance [R3]</label>
                <div class="col-sm-4">
                    <input type="number" class="form-control" id="r3" name="r3" placeholder="">
