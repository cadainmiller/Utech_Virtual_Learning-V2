                </div>
            </div>


            <div class="form-group row">
                <label for="e1" class="col-sm-2 col-form-label">Enter Power Supply Voltage Value [E1]</label>
                <div class="col-sm-4">
                    <input type="number" class="form-control" id="e1" name="e1" placeholder="">
                </div>
                <label for="e1" class=" col-form-label">V</label>
            </div>

            <div class="form-group row">
                <label for="e2" class="col-sm-2 col-form-label">Enter Power Supply Voltage Value [E2]</label>
                <div class="col-sm-4">
                    <input type="number" class="form-control" id="e2" name="e2" placeholder="">
                </div>
                <label for="e2" class=" col-form-label">V</label>
            </div>


            <button type="submit" type="button" class="btn btn-success">Calculate</button>

        </form>
    </div>



    <% include ../partials/footer %>
    <% include ../partials/scripts %>
